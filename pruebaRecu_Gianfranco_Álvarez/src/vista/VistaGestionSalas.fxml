<?import java.lang.*?>
<?import java.net.*?>
<?import java.util.*?>
<?import javafx.scene.*?>
<?import javafx.scene.control.*?>
<?import javafx.scene.layout.*?>

<AnchorPane id="AnchorPane" prefHeight="277.0" prefWidth="351.0" styleClass="mainFxmlClass" xmlns="http://javafx.com/javafx/8" xmlns:fx="http://javafx.com/fxml/1" fx:controller="controlador.VistaGestionSalasController">
    <stylesheets>
        <URL value="@estilo.css" />
    </stylesheets>
   <children>
      <ImageView fitHeight="295.0" fitWidth="360.0" pickOnBounds="true" smooth="false">
         <image>
            <Image url="@IMG/fondoPrincipal.jpg" />
         </image>
      </ImageView>
      <Label fx:id="labeNuevaSala" layoutX="30.0" prefHeight="70.0" prefWidth="301.0" styleClass="fontTitulos" stylesheets="@estilo.css" text="Nueva sala de eventos" textFill="WHITE" />
      <Label fx:id="labelNombreSala" layoutX="49.0" layoutY="84.0" prefHeight="17.0" prefWidth="91.0" styleClass="fontFondoNegro" stylesheets="@estilo.css" text="Nombre sala" />
      <TextField fx:id="textFieldNombreSala" layoutX="150.0" layoutY="80.0" styleClass="big-yellow" stylesheets="@estilo.css" />
      <TextField fx:id="textFieldAforo" layoutX="150.0" layoutY="112.0" styleClass="big-yellow" stylesheets="@estilo.css" />
      <Label fx:id="labelAforo" layoutX="49.0" layoutY="116.0" prefHeight="17.0" prefWidth="91.0" styleClass="fontFondoNegro" stylesheets="@estilo.css" text="Aforo" />
      <Label fx:id="labelPrecio" layoutX="49.0" layoutY="151.0" prefHeight="17.0" prefWidth="91.0" styleClass="fontFondoNegro" stylesheets="@estilo.css" text="Precio" />
