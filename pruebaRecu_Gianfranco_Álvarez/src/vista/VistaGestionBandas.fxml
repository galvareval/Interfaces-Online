      <TextField fx:id="textFieldCache" layoutX="150.0" layoutY="244.0" onKeyPressed="#registrarEnter" styleClass="big-yellow" stylesheets="@estilo.css" />
      <Label fx:id="labelCache" layoutX="49.0" layoutY="248.0" prefHeight="17.0" prefWidth="91.0" styleClass="fontFondoNegro" stylesheets="@estilo.css" text="Caché" />
