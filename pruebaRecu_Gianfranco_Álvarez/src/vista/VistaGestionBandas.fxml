<?import javafx.scene.layout.*?>

<AnchorPane id="AnchorPane" prefHeight="349.0" prefWidth="360.0" styleClass="mainFxmlClass" xmlns="http://javafx.com/javafx/8" xmlns:fx="http://javafx.com/fxml/1" fx:controller="controlador.VistaGestionBandasController">
    <stylesheets>
        <URL value="@estilo.css" />
    </stylesheets>
   <children>
      <ImageView fitHeight="368.0" fitWidth="360.0" pickOnBounds="true" smooth="false">
         <image>
            <Image url="@IMG/fondoPrincipal.jpg" />
         </image>
      </ImageView>
      <Label fx:id="labeNuevaBanda" layoutX="86.0" layoutY="8.0" prefHeight="59.0" prefWidth="222.0" styleClass="fontTitulos" stylesheets="@estilo.css" text="Nueva Banda" textAlignment="RIGHT" textFill="WHITE" />
      <TextField fx:id="textFieldNombreBanda" layoutX="150.0" layoutY="86.0" styleClass="big-yellow" stylesheets="@estilo.css" />
      <Label fx:id="labelNombreBanda" layoutX="49.0" layoutY="90.0" prefHeight="17.0" prefWidth="91.0" styleClass="fontFondoNegro" stylesheets="@estilo.css" text="Nombre banda" />
      <Label fx:id="labelNombreSala" layoutX="49.0" layoutY="129.0" prefHeight="17.0" prefWidth="91.0" styleClass="fontFondoNegro" stylesheets="@estilo.css" text="Sala" />
